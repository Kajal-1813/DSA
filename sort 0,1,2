class solution {
public:
   void sortcolors(vector<int>&nums){
   int n=nums.size();
   for(int i=0;i<n;i++)
   {
        for(int j=i+1;j<n;j++)
        {
          if(nums[i]>nums[j])
          {
            int t=nums[j];
            nums[j]=nums[i];
            nums[i]=t;
          }
        }
   }
}
};
