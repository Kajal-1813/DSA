class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n=nums.size();
        int a;
        sort(nums.begin(),nums.end());
        for(int i=0;i<n;i++)
        {
           if(i!=nums[i])
           {
               a=i;
               break;
           }
                
        }
     return a;
    }
};
